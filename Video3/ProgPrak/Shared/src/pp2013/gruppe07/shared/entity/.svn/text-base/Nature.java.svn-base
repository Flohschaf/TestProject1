package pp2013.gruppe07.shared.entity;

import java.awt.Graphics;

import javax.swing.JComponent;


/**Stellt eine Oberklasse fuer Naturobjekte dar
 * @author Sven
 *  @author Felix,Daniel
 *
 */
public abstract class Nature extends Entity{

	private Type type;
	
	/**
	 * @param x
	 * @param y
	 * @param type
	 * @author Sven
	 */
	public Nature(int x, int y, Type type) {
		super(x, y);
		this.type=type;
		
	}
	
	/**
	 * @return
	 * @author Sven
	 */
	public Type getType(){
		return this.type;
	}
	
	/** Enthaelt den Typ von Naturobjekten
	 * @author Sven
	 */
	public static enum Type
		{TREE,TREE2,WALL}
	
	
}
