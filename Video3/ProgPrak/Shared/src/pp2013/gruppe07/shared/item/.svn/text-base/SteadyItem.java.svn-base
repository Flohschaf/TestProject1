package pp2013.gruppe07.shared.item;

import java.awt.Graphics;

import javax.swing.JComponent;

/**Ist die Oberklasse fuer dauerhafte Items wie Schwerter etc.
 * @author Sven
 *
 */
public abstract class SteadyItem extends Item {

	
	/**Wird auf die default_values der Heros addiert, was den Effect des Items beschreibt.
	 * Erlaeterung der einzelnen Werte in der Klasse shared.entity.InteaktiveEntity im enum values.
	 * @author Sven
	 */
	protected final int[] effects;
	
	public SteadyItem(int cost,int[]effects) {
		super(cost);
		// TODO Auto-generated constructor stub
		this.effects=effects;
	}

	public int[] getEffects() {
		return effects;
	}
	
	
	
}
